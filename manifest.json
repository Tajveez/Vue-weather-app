{
  "name": "Newtab Weather",
  "description": "Base Level Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["6a74bb8d82.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
